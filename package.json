{
  "name": "vx-components",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build --prod --aot --base-href=/VxComponents/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc": "ngc -p src/lib/tsconfig.aot.json",
    "bundleScss": "scss-bundle -e src/demo/styles.scss -d src/demo/assets/bundle.scss",
    "package": "rimraf dist && ngc -p src/lib/tsconfig.aot.json && gulp",
    "pages": "npm run build  &&  angular-cli-ghpages --dir demo",
    "dist": "npm run package && cd dist && npm publish"
  },
  "dependencies": {
    "@angular/animations": "^6.0.3",
